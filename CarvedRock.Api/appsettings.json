{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "Microsoft.Hosting": "Information"
    },
    "Console": {
      "LogLevel": {
        "Default": "Information",
        "CarvedRock": "Debug",
        "Microsoft.AspNetCore": "Warning"
      },
      "FormatterName": "json",
      "FormatterOptions": {
        "SingleLine": true,
        "IncludeScopes": true,
        "TimestampFormat": "HH:mm:ss ",
        "UseUtcTimestamp": true,
        "JsonWriterOptions": {
            "Indented": true
        }
      }          
    }
    // "Debug": {
    //   "LogLevel": {
    //     "Default": "Debug",
    //     "Microsoft.AspNetCore": "Information"
    //   }
    // }
  },
  "AllowedHosts": "*"
}
